{
    "name": "chriswest101/nova-google2fa",
    "description": "This package provides Google2FA to Laravel Nova.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1.0",
        "bacon/bacon-qr-code": "dev-develop",
        "pragmarx/google2fa-laravel": "^2.0",
        "pragmarx/recovery": "^0.2"
    },
    "autoload": {
        "psr-4": {
            "CubeAgency\\NovaGoogle2fa\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CubeAgency\\NovaGoogle2fa\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/chriswest101/BaconQrCode"
        }
    ]
}
